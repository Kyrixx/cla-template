{
  "name": "job-classification",
  "version": "1.0.0",
  "description": "Job Classification - Wizbii",
  "main": "web/dist/app.js",
  "scripts": {
    "clean": "rm -rf web/dist",
    "build": "yarn clean && webpack --config webpack.config.prod.js -p",
    "build:stats": "yarn clean && rm -f stats.json && webpack --profile --config webpack.config.prod.js --json > stats.json && ex -sc '1d7|x' stats.json",
    "build:dev": "yarn clean && webpack -d --progress",
    "watch": "webpack --config webpack.config.js -d --progress --watch",
    "reset": "yarn clean && git checkout web/dist",
    "dev": "webpack-dev-server --host=crm.wizbii.me",
    "lint": "eslint assets/js/**/*.js assets/js/**/*.ts assets/js/**/*.tsx",
    "lint:fix": "eslint --fix assets/js/**/*.js assets/js/**/*.ts",
    "test:coverage": "jest --coverage",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/wizbii/job-classification.git"
  },
  "module": {
    "noParse": "/switchery/"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "setupFiles": [
      "<rootDir>/assets/js/testShim.js",
      "<rootDir>/assets/js/testSetup.ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/wizbii/job-classification#readme",
  "dependencies": {
    "axios": "^0.17.1",
